Algoritmo "semnome"

Var
     N , i , x,soma : inteiro


Inicio
       escreva("Quantos numeros serão digitados?")
       leia(N)
       
       soma <- 0
       
       
       para i de 1 ate N faca
            escreva("Proximo numero:")
            leia(x)
            
            soma <- soma + x
       fimpara
            
       escreval ("Soma =", soma)
            

Fimalgoritmo